int main() {
    // first individually test expressions, then combine
    int x;
    int y;
    x || y;
    x && y;
    x | y;
    x ^ y;
    x & y;
    x == y;
    x != y;
    x < y;
    x > y;
    x <= y;
    x >= y;
    x << y;
    x >> y;
    x + y;
    x - y;
    x * y;
    x / y;
    x % y;
    x ** y;
}